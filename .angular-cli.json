{
  "project": {
    "name": "angularcore"
  },
  "apps": [{
    "root": "",
    "outDir": "dist",
    "assets": [
      "assets",
      "favicon.ico"
    ],
    "index": "index.html",
    "main": "main.ts",
    "polyfills": "polyfills.ts",
    "tsconfig": "tsconfig.json",
    "prefix": "app",
    "styles": [
      "scss/style.scss"
    ],
    "scripts": [
      "~chart.js/dist/Chart.bundle.min.js",
      "~chart.js/dist/Chart.min.js"
    ]
  }],
  "defaults": {
    "styleExt": "scss",
    "component": {
      "inlineStyle": false,
      "inlineTemplate": false
    }
  },
  "warnings": {
    "typescriptMismatch": false
  }
}